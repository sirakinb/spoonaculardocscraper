{
  "title": "Add Custom Foods",
  "group": "Meal Plans",
  "description": "Add custom foods to a user's account. This could be something as simple as \"1 banana\" or even mini recipes like \"1 toast with jam\".",
  "endpoint_url": "https://api.spoonacular.com/food/customFoods/add",
  "endpoint_method": "POST",
  "parameters": {
    "(post body)": {
      "Name": "(post body)",
      "Type": "string",
      "Example": "{\n                            \"title\": \"My little afternoon snack\",\n                            \"servings\": 2\n                          }",
      "Description": "The json representing the custom food."
    },
    "username": {
      "Name": "username",
      "Type": "string",
      "Example": "dsky",
      "Description": "The username."
    },
    "hash": {
      "Name": "hash",
      "Type": "string",
      "Example": "4b5v4398573406",
      "Description": "The private hash for the username."
    }
  },
  "code_examples": [
    {
      "language": "json",
      "code": "{\n    \"id\": 1,\n    \"title\": \"Chickpea Lunchbox\",\n    \"servings\": 1,\n    \"customFoodBoxId\": 123,\n    \"nutrition\": {\n        \"calories\": 300\n    },\n    \"meta\": {\"diets\":[\"vegan\",\"vegetarian\"],\"intolerances\":[\"sesame\",\"soy\"]},\n    \"imageUrl\": \"https://images.unsplash.com/photo-1543353071-c953d88f7033?q=80&w=500&auto=format&fit=crop\"\n}"
    }
  ],
  "quota": "Calling this endpoint requires 5 points. Learn more about quotas."
}